{
	if(!lazy[p]||l==r)return ;
	int mid = (l+r)>>1;
	lazy[p<<1]+=lazy[p];
	lazy[p<<1|1]+=lazy[p];
	sum[p<<1]+=lazy[p]*(mid-l+1);
	sum[p<<1|1]+=lazy[p]*(r-mid);
	lazy[p] = 0;
}
