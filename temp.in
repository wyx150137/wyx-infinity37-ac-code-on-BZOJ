void quick_shai()
{
	phi[1] = mu[1] = 1;
	for(int i = 2;i<M;i++)
	{
		if(!v[i])
		{
			phi[i] = i-1;
			mu[i] = -1;
			prime[++cnt] = i;
		}
		for(int j = 1;j<= cnt&&prime[j]*i<M;j++)
		{
			v[i*prime[j]] = true;
			if(i%prime[j])
			{
				mu[i*prime[j]] = -mu[i];
				phi[i*prime[j]] = phi[i]*(prime[j]-1);
			}else
			{
				mu[i*prime[j]] = 0;
				phi[i*prime[j]] = phi[i]*prime[j];
				break;
			}
		}
	}
	for(int i = 1;i<M;i++)
		phi[i]+=phi[i-1],mu[i]+=mu[i-1];
