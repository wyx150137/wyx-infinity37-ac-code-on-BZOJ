	FFT(a,len,1),FFT(b,len,1);
	for(int i = 0;i<=len;i++)
		c[i]=a[i]*b[i];
	FFT(c,len,-1);
	for(int i = 0;i<=len;i++)
		ans[i]=c[i].x;
}
