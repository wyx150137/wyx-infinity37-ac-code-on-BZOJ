		else if(!x->rs)x= x->ls;
		else
		{
			go_left(x);
			Delete(x->ls,y);
			if(x->ls&&x->ls->key>x->key)
				go_right(x);
		}
	}else if(y<x->val)
		Delete(x->ls,y);
