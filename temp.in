{
	int next,to;
}edge[N],e[N];
int h1[N],h2[N],tot1,tot2,ind[N];
void add1(int x,int y)
{
	edge[++tot1].to = y;
	edge[tot1].next = h1[x];
	h1[x] = tot1;
}
void add2(int x,int y)
{
	ind[y]++;
	e[++tot2].to = y;
	e[tot2].next = h2[x];
	h2[x] = tot2;
}
int dep[N],low[N],dfn,stack[N],top,scc,id[N],W[N],V[N];
int w[N],v[N],n,m;
bool ins[N],vis[N];
void tarjan(int x)
{
	vis[x] = true;
	low[x] = dep[x] = ++dfn;
	stack[++top] = x;
	ins[x] = true;
	for(int i = h1[x];i;i = edge[i].next)
	{
		if(!vis[edge[i].to])
			{tarjan(edge[i].to);low[x] = min(low[x],low[edge[i].to]);}
		else if(ins[edge[i].to])
			low[x] = min(low[x],dep[edge[i].to]);
	}
	if(low[x]==dep[x])
	{
		scc++;
		int tmp;
		do{
			tmp = stack[top--];
			ins[tmp] = false;
			id[tmp] = scc;
			W[scc]+=w[tmp],V[scc]+=v[tmp];
		}while(tmp!=x);
	}
}
