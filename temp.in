		if(e[i].f&&dis[p]+e[i].c==dis[e[i].to]&&(!visit[e[i].to]||e[i].to==t))
		{
			int tof = getflow(e[i].to,s,t,min(last,e[i].f));
			e[i].f-=tof;
			e[i^1].f+=tof;
			last-=tof;
			if(!last)
