		e[++tot].to = x;e[tot].next = head[y];head[y] = tot;
	}
	void dp(int x,int fa)
	{
		f[x][1]=1;
		if(x<=Cactus::n)
		{
			for(int i = head[x];i;i=e[i].next)
				if(e[i].to!=fa)
				{
					dp(e[i].to,x);
					if(e[i].to>Cactus::n)continue;
					f[x][0]+=max(f[e[i].to][0],f[e[i].to][1]);
					f[x][1]+=f[e[i].to][0];
				}
