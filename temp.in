	for(int i = r;i>l;i--)
		if(pre[i]==l&&r-l+1>=(pre[i-1]-l+1)*2)
			{x = pre[i-1]-l+1;break;}
	if(!check(x,l,r)&&(r-pre[r])*2>=r-l+1&&pre[r]>=l)x = r-pre[r];
	if(check(x,l,r)&&judge(l,l+x-1)&&judge(l+x,r))return true;
