bool check(int x,int l,int r)
{
	if(x<=0)return false;
	for(int i = l+x;i<= r;i++)
	{
		if((i-l)%x!=pre[i]-l)return false;
		if(e[fi[i]].x==i&&e[fi[i]].y<l+x&&fi[i]<=m)return false;
	}
	return true;
