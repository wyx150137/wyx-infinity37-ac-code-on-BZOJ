void dfs1(int x,int f)
{
	fa[x] = f;
	size[x]++;
	dep[x] = dep[f]+1;
	int p = fa[x];
	for(int i =1;i<= 150&&p;p = fa[p],i++)sum[x][i] = sum[p][i]+a[x];
	for(int i = head[x];i;i = e[i].next)
	{
		if(e[i].to!=f)
		{
			dfs1(e[i].to,x);
