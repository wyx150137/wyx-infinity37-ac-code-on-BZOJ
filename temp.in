		while(top2>1 && Slope(q[stk2[top2-1]].a,q[i].a)<Slope(q[stk2[top2-1]].a,q[stk2[top2]].a)+eps)
			top2--;
		stk2[++top2] = i;
	}
	int j = 1;
	for(int i = mid+1;i<= r;i++)
	{
		if(q[i].type==0)continue;
		if(q[i].a.y<eps)
		{
			while(top1>1&&Slope(q[stk1[top1-1]].a,q[stk1[top1]].a)<q[i].k)
				top1--;
