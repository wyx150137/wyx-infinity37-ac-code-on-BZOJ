	scanf("%d",&n);
	for(int i = 1;i<= n;i++)
		scanf("%d",&a[i]);
	for(int i=1;i<=n;i++)
		build(1,1,n,i,a[i]);
	int ans = 0;
	for(int i=1;i<=n;i++)
		ans+=get_bigger(1,1,n,1,i-1,a[i]);
	int x,y;
	scanf("%d",&m);
	while(m--)
	{
