				ap[q[i].id]=false;
		}else
		{
			while (j<top2 && Slope(q[stk2[j]].a,q[stk2[j+1]].a)<q[i].k)
				j++;
            if (j<=top2 && Distance(q[stk2[j]].a,q[0].a)<Distance(q[stk2[j]].a,q[i].a)) 
