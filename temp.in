	sum[ls[p]]+=(mid-l+1)*lazy[p];
	sum[rs[p]]+=(r-mid)*lazy[p];
	lazy[p] = 0;
}
void update(int &p,int l,int r,int a,int b)
{
	if(!p)p = ++tot;
	push_down(p,l,r);
	if(l>=a&&r<=b)
	{
		sum[p]+=r-l+1;
		lazy[p]++;
		return ;
	}
	int mid = (l+r)>>1;
	if(a<=mid)update(ls[p],l,mid,a,b);
	if(b >mid)update(rs[p],mid+1,r,a,b);
	sum[p] = sum[ls[p]]+sum[rs[p]];
