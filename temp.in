void update(int x)
{
        if (!x) return;
        la[x]=max(la[c[x][0]],sum[c[x][0]]+key[x]+max(0,la[c[x][1]]));
        ra[x]=max(ra[c[x][1]],sum[c[x][1]]+key[x]+max(0,ra[c[x][0]]));
        ma[x]=max(max(ma[c[x][0]],ma[c[x][1]]),key[x]+max(0,ra[c[x][0]])+max(0,la[c[x][1]]));
        sum[x]=sum[c[x][0]]+sum[c[x][1]]+key[x];
        size[x]=size[c[x][0]]+size[c[x][1]]+1;
}

void reverse(int x)
{
        if (!x) return;
        swap(c[x][0],c[x][1]);
        swap(la[x],ra[x]);
        rev[x]^=1;
}

void recover(int x,int z)
{
        if (!x) return ;
        key[x]=cov[x]=z;
        sum[x]=size[x]*z;
        la[x]=ra[x]=ma[x]=max(z,sum[x]);
}

void down(int x)
{
        if (!x) return;
        if (rev[x])
        {
                reverse(c[x][0]);
                reverse(c[x][1]);
                rev[x]=0;
        }
        if (cov[x]!=-inf)
        {
                recover(c[x][0],cov[x]);
                recover(c[x][1],cov[x]);
                cov[x]=-inf;
        }
}

