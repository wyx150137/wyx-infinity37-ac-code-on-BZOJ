	scanf("%d",&n);
	for(int i = 1;i<= n;i++)
	{
		scanf("%d",&x);
		if(i==1){ans+=x;Insert(root,x);}
		else
		{
			int tmp = get_pre(root,x),minn = INF;
			if(tmp!=-INF) minn = x-tmp;
			tmp = get_suc(root,x);
			if(tmp!=INF)minn = min(minn,tmp-x);
			ans+=minn;
			Insert(root,x);
		}
	}
