	for(int i = 2;i<N;i++)
	{
		if(!vis[i])
		{
			prime[++cnt] = i;
			mu[i] = -1;
		}
		for(int j = 1;j<= cnt&&i*prime[j]<N;j++)
		{
			vis[i*prime[j]]=true;
			if(i%prime[j]==0)break;
			else
				mu[i*prime[j]]=-mu[i];
		}
	}
	for(int i = 1;i<= k;i++)
		f[i] = f[i-1]+(gcd(i,k)==1),g[i]=g[i-1]+(gcd(i,k)==1)*mu[i];
	for(int i = 2;i<= k;i++)if(k%i==0&&mu[i])stack[++top]=i;
	for(int i = 1;i<N;i++)mu[i]+=mu[i-1];
}
