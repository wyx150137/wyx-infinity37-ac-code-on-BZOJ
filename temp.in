{
	Push_down(x);
	while(!is_root(x))
	{
		int y = fa[x],z = fa[y];
		if(!is_root(y))
		{
			if((son[y][0]==x)^(son[z][0]==y))rotate(x);
			else rotate(y);
		}
		rotate(x);
	}
	Push_up(x);
}
