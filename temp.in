	int NewNode(int x)
	{
		int ret = Addpoint();
		b[ret] = 1;
		val[ret] = x;
		size[ret] = sz[ret] = 1;
		v[ret] = Node(x,0);
		trie.Insert(root[ret],x,20,1);
