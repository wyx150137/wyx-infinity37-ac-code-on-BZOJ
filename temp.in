void get_fac(ll x)
{
	stack[0]=0;
	for(ll i = 1;i*i<=x;i++)
		if(x%i==0)
		{
			stack[++stack[0]] = i;
			if(i*i!=x)stack[++stack[0]]=x/i;
		}
	sort(stack+1,stack+stack[0]+1);
}
