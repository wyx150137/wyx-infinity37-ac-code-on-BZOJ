	size[p] = size[p<<1]+size[p<<1|1];
}
void build(int p,int l,int r)
{
	if(l==r)
	{
		sum[p] = size[p] = 0;
		return ;
	}
	int mid = (l+r)>>1;
	build(p<<1,l,mid);
	build(p<<1|1,mid+1,r);
