{
	if(len==1)
	{
		b[0] = quick_pow(a[0],mod-2);
		return ;
	}
	Get_Inv(a,b,len>>1);
	static int tmp[N];
	memcpy(tmp,a,sizeof(int)*len);
	memset(tmp+len,0,sizeof(int)*len);
	NTT(tmp,len<<1,1);
	NTT(b,len<<1,1);
	for(int i = 0;i<len<<1;i++)b[i] = (ll)b[i]*(2-(ll)b[i]*tmp[i]%mod+mod)%mod;
	NTT(b,len<<1,-1);
	memset(b+len,0,sizeof(ll)*len);
}
