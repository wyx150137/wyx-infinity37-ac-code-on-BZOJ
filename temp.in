{
	memset(v,0,sizeof(v));
	memset(dis,0x3f,sizeof(dis));
	Q.push(sta);
	v[sta] = true;
	dis[sta] = 0;
	while(!Q.empty())
	{
		int x = Q.front();
		Q.pop();
		v[x] = false;
		for(int i = head[x];i;i = e[i].next)
		{
			if(dis[x]+1<dis[e[i].to]||(dis[x]+1==dis[e[i].to]&&x<step[sta][e[i].to]))
			{
				step[sta][e[i].to] = x;
				dis[e[i].to] = dis[x]+1;
