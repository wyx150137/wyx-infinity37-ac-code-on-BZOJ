Splay :: Splay(int x)
{
	ls = rs = fa = null;
	val = sum = x;
	if(x)size = 1;else size = 0;
	add = 0;rev = false;times = 1;
}
void Splay :: reverse()
{
	if(this==null)return ;
	swap(ls,rs);
	rev ^= 1;
}
void Splay :: Add(int x)
{
	if(this==null)return ;
	(val+=x)%=mod;
	(sum+=(LL)x*size%mod)%=mod;
	(add+=x)%=mod;
}
void Splay :: Times(int x)
{
	if(this==null)return ;
	times=(LL)times*x%mod;
    val=(LL)val*x%mod;
    add=(LL)add*x%mod;
    sum=(LL)sum*x%mod;
}
