	for(int i = 1;i<= m;i++)
	{
		scanf("%d",&opt);
		if(opt==1){scanf("%d",&x);root = x;}
		else if(opt==2){scanf("%d%d%lld",&x,&y,&z);get_update(x,y,z);}
