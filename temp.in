void push_down(int p,int l,int r)
{
	if(l==r){lazy[p] = 0;return ;}
	int mid = (l+r)>>1;
	lazy[p<<1] += lazy[p];
	lazy[p<<1|1] +=lazy[p];
	sum[p<<1] += lazy[p]*(mid+1-l);
	sum[p<<1|1]+=lazy[p]*(r-mid);
	lazy[p] = 0;
}
