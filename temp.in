{
	F[p] = -1;
	if(l==r)
	{
		lmx[p] = rmx[p] = f[l];
		mx[p] = f[l]+sb[l];
		return ;
	}
	int mid = (l+r)>>1;
	build(p<<1,l,mid);
	build(p<<1|1,mid+1,r);
	Push_up(p);
}
void Update(int p,int l,int r,int a,int b,int v)
{
	if(l>=a&&r<=b)
