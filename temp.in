void dfs1(int x)
{
	size[x] = 1;
	dep[x] = dep[fa[x]]+1;
	for(int i = head[x];i;i=e[i].next)
		if(e[i].to!=fa[x])
		{
			fa[e[i].to] = x;
			dfs1(e[i].to);
			if(size[e[i].to]>size[son[x]])son[x]=e[i].to;
			size[x]+=size[e[i].to];
		}
}
