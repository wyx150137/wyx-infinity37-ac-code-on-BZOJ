        B=mul(B,x,M);
    }
    for(int i=1;i<=m;++i)
    {
        x=a[i];
        if(fs[0]==2)for(;~x&1;x>>=1,--ts[0]);
        for(int j=(fs[0]==2);j<fp;++j)
        {
            for(ll p=fs[j],y=x/p;y*p==x;x=y,y=y/p,--ts[j]);
