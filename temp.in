{
	int m,x,y;
	UI c;
	scanf("%d%d",&n,&m);
	for(int i = 1;i<= m;i++)
	{
		scanf("%d%d%u",&x,&y,&c);
		add(x,y,c);
		if(x!=y)add(y,x,c);
	}
	double t_ans = 0;
	for(int pos = 0;pos<32;pos++)
	{
		memset(f,0,sizeof(f));
		for(int i = 1;i< n;i++)
			for(int j = head[i];j;j = e[j].next)
			{
				if(e[j].val&(1<<pos))f[i][e[j].to]+=1.0,f[i][n+1]+=1.0;
				else f[i][e[j].to]-=1.0;
			}
		for(int i = 1;i< n;i++)
			f[i][i]+=cnt[i];
		memset(ans,0,sizeof(ans));
		work();
		t_ans+=ans[1]*(1<<pos);
	}
	printf("%.3f\n",t_ans);
	return 0;
}
