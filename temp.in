inline LL muti_mod(LL a,LL b,LL c)
{
    a%=c;
    b%=c;
    LL ret=0;
    while(b)
    {
        if(b&1){ret+=a;if(ret>=c)ret-=c;}
        a<<=1;
        if(a>=c)a-=c;
        b>>=1;
    }
    return ret;
}
inline LL pow_mod(LL x,LL n,LL mod)
{
    if(n==1)return x%mod;
    int bit[50],k=0;
    while(n)
    {
        bit[k++]=n&1;
        n>>=1;
    }
    LL ret=1;
    for(;k>0;k--)
    {
        ret=muti_mod(ret,ret,mod);
        if (bit[k-1]==1) ret=muti_mod(ret,x,mod);
    }
    return ret;
}
