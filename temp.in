{
	if(this==null)return ;
	size = ls->size+rs->size+1;
	sum = (ls->sum+rs->sum+val)%mod;
}
void Splay :: push_down()
{
	if(this==null)return ;
	if(this==fa->ls||fa->rs==this)
		fa->push_down();
