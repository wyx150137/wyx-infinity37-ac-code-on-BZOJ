			res+=L[t-num[i]];
			if (cur == 1) continue;
		}
	if (p <= end[1] || p >= st[tot]) return;
    res += ans[t];
}

void solve()
{
	for(int i = 1;i<= n;i++)R[num[i]]++;
	for(int i = 1;i<= tot;i++)
	{
