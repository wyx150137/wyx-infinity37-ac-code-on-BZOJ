double slope(const P &a,const P &b)
{
	if(fabs(a.x-b.x)<eps)return -INF;
	else return (b.y-a.y)/(b.x-a.x);
}
double slope(const P &a)
{
	return -sqrt(a.y/a.x);
}
double calc(const P &a,double k)
{
	if(k>-eps)return INF;
	else return a.x+a.y-a.x*k-a.y/k;
}
