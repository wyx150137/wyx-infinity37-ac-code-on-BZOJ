		x = fa[x];
	}
	while(top)
	{
		x = sta[top--];
		Push_down(x);
	}
}
void splay(int x,int y)
{
	relax(x,y);
	while(fa[x]!=y)
	{
		if(fa[fa[x]]!=y)
		{
			if(pd(x)==pd(fa[x]))rotate(fa[x]);
			else rotate(x);
		}
		rotate(x);
	}
}
