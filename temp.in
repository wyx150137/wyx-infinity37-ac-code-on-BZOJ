{
	double x,y;
	Point(){}
	Point(double x,double y):x(x),y(y){}
	Point operator+(const Point &a)const
	{
		return Point(x+a.x,y+a.y);
	}
	Point operator-(const Point &a)const
	{
		return Point(x-a.x,y-a.y);
	}
	double operator*(const Point &a)const
	{
		return x*a.y-y*a.x;
	}
	double operator^(const Point &a)const
	{
		return x*a.x+y*a.y;
	}
}mtr[N],req[N];
struct Line
{
	Point p,v;
	Line(){}
	Line(const Point &a,const Point &b):p(a),v(b){}
};
