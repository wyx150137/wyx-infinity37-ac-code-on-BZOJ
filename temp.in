			h[i][j] = h[i][j-1]*sed+s[i][j];
			h0[i][j] = (h0[i][j-1]*sed+s[i][j])%mod;
		}
	}
	pw2[0] = pws[0] = pw0[0] = 1;
	for(int i = 1;i<= m;i++)
	{
		pw2[i] = pw2[i-1]*0.5;
