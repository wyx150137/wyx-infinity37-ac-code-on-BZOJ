{
	for(int i = 2;i<=500;i++)
	{
		if(!vis[i])
			prime[cnt++] = i;
		for(int j = 0;j<cnt&&i*prime[j]<=500;j++)
		{
			vis[i*prime[j]]=true;
			if(i%prime[j]==0)break;
		}
