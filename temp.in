{sum[p]=sum[ch[p][0]]+val[p]+sum[ch[p][1]];}
void rotate(int x)
{
	int y = fa[x];
	int d = ch[y][1]==x;
	fa[x] = fa[y];
	if(fa[y])ch[fa[y]][pd(y)] = x;
	ch[y][d]=ch[x][d^1];
