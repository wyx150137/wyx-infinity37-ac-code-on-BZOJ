		int flag =((x<y&&a[x]>a[y])||(x>y&&a[x]<a[y]));
		ans+=flag;
		ans-=get_bigger(1,1,n,1,x-1,a[x])+get_smaller(1,1,n,x+1,n,a[x]);
		ans-=get_bigger(1,1,n,1,y-1,a[y])+get_smaller(1,1,n,y+1,n,a[y]);
		update(1,1,n,x,a[x],a[y]);update(1,1,n,y,a[y],a[x]);
		swap(a[x],a[y]);
