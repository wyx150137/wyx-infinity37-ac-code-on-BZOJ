		else
		{
			int tmp = get_pre(root,x),minn = INF;
			if(tmp!=-INF) minn = x-tmp;
			tmp = get_suc(root,x);
			if(tmp!=INF)minn = min(minn,tmp-x);
			ans+=minn;
