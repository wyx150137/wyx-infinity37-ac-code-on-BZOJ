	while (!q.empty()) q.pop();
	for (int i=1; i<=m; i++)
		for (int j=1; j<=n; j++)
			for (int k=0; k<=len; k++)
			{
				vis[i][j][k]=0; 
				dis[i][j][k]=inf;
			}
