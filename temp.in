	{
		static int q[N];
		int hd = 1,tl = 0;
		q[++tl] = x;vis[x] = true;
		while(hd<=tl)
		{
			int cur = q[hd++];
			stack[++top] = cur;
			for(int i = head[cur];i;i=e[i].next)
				if(!vis[e[i].to])
					vis[e[i].to]=true,q[++tl] = e[i].to;
		}
	}
	void Solve()
	{
		for(int i = 1;i<= n;i++)
			add(i,aim[i]);
		for(int i = 1;i<= n;i++)
			if(!vis[i])
			{
				top = 0;
				bfs(i);
				if(top==1)ans2++;
				else
				{
					int cnt = 0,size = top;
					while(top)
						cnt+=degree[stack[top--]]==0;
					ans2+=size-max(1,cnt);
				}
			}
	}
}
