void calc(int x)
{
	int tmp = x;
	for(int i = 0;i<8;i++)
		if(tmp%prime[i]==0)
		{
			num[x].l|=1<<i;
			while(tmp%prime[i]==0)tmp/=prime[i];
		}
	num[x].p = tmp;
}
