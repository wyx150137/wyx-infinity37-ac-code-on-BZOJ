	{
		scanf("%lld",&x);
		heap.push(Heap(x,1));
	}
	if((n-1)%(k-1))nn+=(k-1)-(n-1)%(k-1);
	for(int i = n+1;i<= nn;i++)
		heap.push(Heap(0,1));
	while(nn>1)
	{
		ll s1 = 0;int s2 = 0;
