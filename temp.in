	{
		Seg res={a.x,b.y,a.z+b.z};
		int j = (a.y||b.x)?-1:0;
		if(b.s1==j)res.s1=a.s1,res.s2=a.s2;
		else 
			res.s1 = b.s1+a.z,res.s2=(b.s2!=j)?b.s2+a.z:a.s1;
		if(a.t1==j)res.t1=b.t1,res.t2=b.t2;
		else
			res.t1 = a.t1+b.z,res.t2=(a.t2!=j)?a.t2+b.z:b.t1;
		return res;
	}
