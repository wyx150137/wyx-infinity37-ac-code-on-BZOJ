				if(last[x]==1)g[x] = 1;
				else g[x] = (t[last[x]]==t[x]?-g[last[x]]:0);
				break;
			}
			last[x] = i;t[x] = 1;
			g[x] = (t[i]==1?-g[i]:0);
		}
	}
	for(int i = 1;i<N;i++)
		sum[i] = sum[i-1]+g[i];
