	for(int i = 2;i<= n;i++)
		if(f[i]==f[i-1])ls[i] = ls[i-1];
		else ls[i] = i-1;
	ls[0] = -1;
	int ans = 0,now = 0;
	for(int i = n;i>= 2;i--)
	{
		now = gcd(now,a[i]);
		for(int j = i-2;j>= 0;j=ls[j])
			ans = (ans+(ll)(j-ls[j])*gcd(now,f[j])%mod)%mod;
		if(i==n)ans = (ans-now+mod)%mod;
	}
	for(int i = 2;i<= n-1;i++)
		(ans+=f[i])%=mod;
	printf("%d\n",ans);
	return 0;
}

