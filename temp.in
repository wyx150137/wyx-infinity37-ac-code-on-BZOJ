{
	rev[p]^=1;
	swap(son[p][0],son[p][1]);	
}
void Multi(int p,int m)
{
	if(!p)return ;
	sum[p] = (ll)sum[p]*m%mod;
	val[p] = (ll)val[p]*m%mod;
	add[p] = (ll)add[p]*m%mod;
	mul[p] = (ll)mul[p]*m%mod;
}
