void get_fac(ll p)
{
	num = 0;
	for(ll i = 2;i*i<= p;i++)
		if(p%i==0)
		{
			prime[++num] = i;
			while(p%i==0)p/=i;
		}
	if(p>1)prime[++num] = p;
