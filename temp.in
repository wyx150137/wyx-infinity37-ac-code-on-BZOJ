{
	for(int i = 1;i<= n;i++)
		for(int j = h1[i];j;j = edge[j].next)
			if(id[i]!=id[edge[j].to]&&!map[id[i]][id[edge[j].to]])
			{
				add2(id[i],id[edge[j].to]);
				map[id[i]][id[edge[j].to]] = true;
			}
}
