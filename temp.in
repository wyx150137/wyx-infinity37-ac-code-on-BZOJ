	{
		int mid = (l+r)>>1,sum=0;
		for(int i = 0;i<s1;i++)sum+=a[a[T[i]].l].sum;
		for(int i = 0;i<s2;i++)
			if(P[i]>=l&&P[i]<=mid)
				sum++;
		if(K<sum)
		{
			for(int i = 0;i<s1;i++)T[i]=a[T[i]].l;
			r=mid;
		}else
		{
			for(int i = 0;i<s1;i++)T[i]=a[T[i]].r;
			l = mid+1;K-=sum;
		}
	}
	T.clear();P.clear();
	return l;
}
