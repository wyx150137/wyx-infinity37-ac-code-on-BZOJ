ll quick_pow(ll x,ll y,ll p=INF)
{
	ll res = 1;
	while(y)
	{
		if(y&1)res = res*x%p;
		x = x*x%p;
		y>>=1;
	}
	return res;
}
bool check(int x,ll p,ll k)
