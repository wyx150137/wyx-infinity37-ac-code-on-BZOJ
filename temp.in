{
	if(this==null)return ;
	size = ls->size+rs->size+1;
	sum = (ls->sum+rs->sum+val)%mod;
}
void Splay :: push_down()
{
	if(this==null)return ;
	if(this==fa->ls||fa->rs==this)
		fa->push_down();
	if(times^1)
	{
		ls->Times(times);
		rs->Times(times);
		times = 1;
	}
	if(add)
	{
		ls->Add(add);
		rs->Add(add);
		add = 0;
	}if(rev)
	{
		ls->reverse();
		rs->reverse();
		rev = false;
	}
}
