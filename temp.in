bool judge(int l,int r)
{
	if(l==r)return true;
	int x=0;
	for(int i = l;i<= r;i++)
	{
		if(pre[i]>=l)continue;
		for(;fi[i]<=m&&e[fi[i]].x==i&&e[fi[i]].y<l;fi[i]++);
		if(fi[i]<=m&&e[fi[i]].x==i){pre[i]=e[fi[i]].y;++fi[i];}
	}
