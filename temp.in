    if(mr(n))
    {
        fs[fp++]=n;
        return;
    }
    for(int c=12347;;++c)
    {
        ll a=get(n,c);
        if(a!=n)
        {
            ll b=gcd(a,n/a);
            calc(a/b);
            calc(n/a/b);
            calc(b);
            return;
        }
    }
}
ll cal(ll*b,ll*a,ll M)
{
    fp=0;
    calc(M);
    std::sort(fs,fs+fp);
    fp=std::unique(fs,fs+fp)-fs;
    ll phi_M=M;
    for(int i=0;i<fp;++i)ts[i]=0,phi_M=phi_M/fs[i]*(fs[i]-1);
