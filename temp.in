	bool operator ==(const Point &a)const
	{
		return (a.x==x&&a.y==y);
	}
	Point operator -(const Point &a)const
	{
		return Point(x-a.x,y-a.y);
	}
	ll operator *(const Point &a)const
	{
		return x*a.y-y*a.x;
	}
