	}
	Insert(root[p],0,N,val,1);
	int lsize = a[root[ls[p]]].sum;
	if(x<=lsize)Insert(ls[p],x,val);
	else Insert(rs[p],x-lsize-1,val);
	if(a[root[p]].sum*beta>max(a[root[ls[p]]].sum,a[root[rs[p]]].sum))
	{
