	{
		for(int p = root[k];p;p=root[k])
		{
			Push_down(p);
			if(val[p]>=h[k])return ;
			ans[id[p]] = level[p];
			death[k]++;
			root[k] = merge(ls,rs);
		}
	}
	void Go(int k)
	{
		Push_down(root[k]);
		if(a[k])mul[root[k]]=v[k];
		else add[root[k]]=v[k];
		exper[root[k]]++;
		root[fa[k]] = merge(root[fa[k]],root[k]);
	}
