	{
		return (a.len==len&&a.midp==midp);
	}
	bool operator<(const Line &a)const
	{
		if(a.len==len)
		{
			if(a.midp.x==midp.x)
				return midp.y<a.midp.y;
			return midp.x<a.midp.x;
		}
		return len<a.len;
	}
}line[(N*N)>>1];
ll dis(const Point &p1,const Point &p2)
{
	return (p1.x-p2.x)*(p1.x-p2.x)+(p1.y-p2.y)*(p1.y-p2.y);
