	{
		int tmp = 1;
		for(int i = x;i!=x||tmp==1;i = a[i],tmp++)
			on_ring[i] = id,pos[i] = tmp,root[i] = i;
		size[id] = --tmp;
		return ;
	}
	if(!v[a[x]])
		dfs(a[x],id);
	if(!on_ring[x])
	{
		fa[x][0] = a[x];
		dep[x] = dep[a[x]]+1;
		root[x] = root[a[x]];
	}
}
