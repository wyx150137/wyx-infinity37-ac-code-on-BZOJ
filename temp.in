	B[n+1] = 1;
	for(int i = 1,l;i<= deg;i++)
	{
		for(int j = (l=i)+1;j<=deg;j++)if(fabs(f[j][i])>fabs(f[l][i]))l = j;
		for(int j = i;j<= deg;j++)swap(f[i][j],f[l][j]);
