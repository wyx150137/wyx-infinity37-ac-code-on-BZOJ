	int mid = (l+r)>>1;
	if(pos<=mid)Insert(ls[p],l,mid,pos);
	else Insert(rs[p],mid+1,r,pos);
	sum[p] = sum[ls[p]]+sum[rs[p]];
}
int merge(int x,int y,int l,int r)
{
	if(!x||!y)return x+y;
	if(l==r)
	{
		sum[x] = sum[x]+sum[y];
		return x;
	}
	int mid = (l+r)>>1;
	ls[x] = merge(ls[x],ls[y],l,mid);
	rs[x] = merge(rs[x],rs[y],mid+1,r);
