ll Gauss()
{
	ll res = 1,f = 1;
	for(int i = 1;i<n;i++)
		for(int j = 1;j<n;j++)
			(d[i][j]+=mod)%=mod;
	for(int i = 1;i<n;i++)
	{
		for(int j = i+1;j<n;j++)
		{
			ll A = d[i][i],B = d[j][i];
			while(B)
			{
				ll C = A/B;
				A%=B;
				swap(A,B);
				for(int k = i;k<n;k++)
					d[i][k] = (d[i][k]-C*d[j][k]%mod+mod)%mod;
				for(int k = i;k<n;k++)
					swap(d[i][k],d[j][k]);
				f = -f;
			}
		}
		if(!d[i][i])return 0;
		res = res*d[i][i]%mod;
	}
