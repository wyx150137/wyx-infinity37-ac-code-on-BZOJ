	{
		static int q[N];
		int hd = 1,tl = 0;
		memcpy(degree,::degree,sizeof(degree));
		for(int i = 1;i<= n;i++)if(!degree[i])q[++tl]=i;
		while(hd<=tl)
		{
			int x = q[hd++];
			vis[x] = true;
			if(!dead[aim[x]])
			{
				dead[aim[x]] = vis[aim[x]] = true;ans1++;
				if(!--degree[aim[aim[x]]])
					q[++tl] = aim[aim[x]];
			}
		}
		for(int i =1;i<= n;i++)
			if(!vis[i])
			{
				int cnt = 0,x = i;
				while(!vis[x])
				{
					vis[x] = true;
					cnt++;
					x = aim[x];
				}
				ans1+=(cnt+1)>>1;
			}
	}
}
namespace Ans2
{
	struct E
	{int next,to;}e[N<<1];
	int head[N],tot;
	void add(int x,int y)
	{
		e[++tot].to = y;e[tot].next = head[x];head[x] = tot;
		e[++tot].to = x;e[tot].next = head[y];head[y] = tot;
	}
