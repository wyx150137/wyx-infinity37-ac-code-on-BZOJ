	{
		Martix ans(S.w,h);
        for(int i = 1;i<= ans.h;i++)
            for(int j = 1;j<= ans.w;j++)
                for(int k = 1;k<= S.h;k++)
                    (ans.map[i][j]+=map[i][k]*S.map[k][j])%=mod;
		return ans;
	}
};
Martix quick_pow(LL y,const Martix &S) 
{
	if(y==0)
	{
		Martix one(3,3);
		for(int i = 0;i<= 3;i++)
			one.map[i][i] = 1;
		return one;
	}
	Martix tmp = quick_pow(y/2,S);
	if(y&1)return tmp*tmp*S;
	else return tmp*tmp;	
}
