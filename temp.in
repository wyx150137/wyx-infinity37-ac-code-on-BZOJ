		ny[i] = pow(jc[i],mod - 2);
	}
}

int getW(int x1,int y1)
{
	return (ll)jc[x1 + y1] % mod * ny[x1] % mod * ny[y1] % mod * toB[x1] % mod * toA[y1] % mod;
}

