	{return cp(a.x+b.x,a.y+b.y);}
	friend cp operator -(const cp &a,const cp &b)
	{return cp(a.x-b.x,a.y-b.y);}
	friend cp operator *(const cp &a,const cp &b)
	{return cp(a.x*b.x-a.y*b.y,a.x*b.y+a.y*b.x);}
