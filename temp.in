{
	size[p] = size[ch[p][0]]+1+size[ch[p][1]];
	int ls = mx[ch[p][0]],rs = mx[ch[p][1]];
	if(val[ls]>=val[p]&&val[ls]>=val[rs])mx[p] = ls;
