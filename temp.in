struct Treap
{
	Treap *ls,*rs;
	int key,cnt,size;
	LL val;
	Treap(int x);
	void push_up();
}*root0,*root1;
Treap :: Treap(int x)
{
	ls = rs = NULL;
	key = rand();
	val = x;
	cnt = size = 1;
}
void Treap :: push_up()
{
	size = cnt;
	if(ls)size+=ls->size;
	if(rs)size+=rs->size;
}
void go_left(Treap *&x)
{
	Treap *y = x->rs;
	x->rs = y->ls;
	y->ls = x;
	x = y;
	x->ls->push_up();
	x->push_up();
}
void go_right(Treap *&x)
{
	Treap *y = x->ls;
	x->ls = y->rs;
	y->rs = x;
	x = y;
	x->rs->push_up();
	x->push_up();
}
void insert(Treap *&x,int y)
{
	if(!x){x = new Treap(y);return ;}
	if(x->val==y)x->cnt++;
	else if(y<x->val)
	{
		insert(x->ls,y);
		if(x->ls->key<x->key)
			go_right(x);
	}else
	{
		insert(x->rs,y);
