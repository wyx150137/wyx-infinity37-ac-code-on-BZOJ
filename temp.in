int get_mxf(int s,int t,int mx)
{
	if(s==t)return mx;
	int last = mx;
	for(int i = head[s];i;i=e[i].next)
		if(e[i].f&&d[e[i].to]==d[s]-1)
		{
			int tof = get_mxf(e[i].to,t,min(e[i].f,last));
