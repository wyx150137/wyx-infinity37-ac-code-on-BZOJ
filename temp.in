	int cur = n / ((LL)1<<(cnt-1));
    int pre = cur;
    n %= ((LL)1<<(cnt-1));
	for(int j = cnt-1;j>=1;j--)
	{
		cur = n/((LL)1<<(j-1));
		if(cur)
			ans+=f[j][0];
		if(cur&&pre)break;
		pre = cur;
		n%=((LL)1<<(j-1));
	}
}
