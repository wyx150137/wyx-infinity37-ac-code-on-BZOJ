int F(int x)
{
	LL ans = 0;
	LL x2 = (LL)x*x%mod;LL x3 = x2*x%mod;
	ans = (((ans+(LL)sa*x3%mod)%mod+(LL)sb*x2%mod)%mod+(LL)sc*x%mod)%mod+sd;
	ans = ans%mod;
	return ans;
