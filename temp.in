struct Martix
{
	int h,w;
	LL map[4][4];
	Martix(int x,int y)
	{
		h = x;
		w = y;
		memset(map,0,sizeof(map));
	}
	Martix()
	{
		h = w = 0;
		memset(map,0,sizeof(map));
	}
