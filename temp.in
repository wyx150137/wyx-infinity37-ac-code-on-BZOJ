	while(x==x->fa->ls||x==x->fa->rs)
	{
		y = x->fa,z = y->fa;
		if(x==y->ls)
		{
			if(y==z->ls)go_right(y);
			go_right(x);
		}else 
		{
			if(y==z->rs)go_left(y);
			go_left(x);
		}
	}
}
