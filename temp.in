			{
				g[i][0]+=max(g[i+1][0],g[i+1][1]);
				g[i][1]+=g[i+1][0];
			}
			f[fa][0] = g[1][0];
			for(int i = 1;i<top;i++)
			{
				g[i][0]-=max(g[i+1][0],g[i+1][1]);
				g[i][1]-=g[i+1][0];
			}
			g[top][1] = -INF;
			for(int i = top-1;i>=1;i--)
			{
				g[i][0]+=max(g[i+1][0],g[i+1][1]);
