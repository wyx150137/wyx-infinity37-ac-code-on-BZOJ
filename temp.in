	{
		ll ans = dis_sum1[x];
		for(int i = x;fa[i];i=fa[i])
		{
			int dis = Tree::getdis(x,fa[i]);
			ans+=dis_sum1[fa[i]]-dis_sum2[i];
			ans+=(sum1[fa[i]]-sum2[i])*dis;
		}
		return ans;
	}
