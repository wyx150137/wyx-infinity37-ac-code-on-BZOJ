		inv[i] = (ll)(mod-mod/i)*inv[mod%i]%mod;
}
ll C(ll n,ll m)
{
	ll res = 1;
	n%=mod;
	for(int i = 1;i<= m;i++)
		(res*=(ll)(n-i+1)*inv[i]%mod)%=mod;
	return res;
}
