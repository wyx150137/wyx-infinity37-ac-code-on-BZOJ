	}
	fa[y] = x,fa[x] = z,fa[ch[x][d^1]] = y;
	ch[y][d] = ch[x][d^1],ch[x][d^1] = y;
	Push_up(y),Push_up(x);
}
void splay(int x)
{
	Push_down(x);
	while(!is_root(x))
	{
		int y = fa[x],z = fa[y];
