struct AC_Machine
{
	int ch[LEN][26],sum[LEN],fail[LEN],q[LEN];
	int cnt,h,t;
	AC_Machine()
	{
		cnt = 1;
		for(int i = 0;i<26;i++)
			ch[0][i]=1;
	}
	void insert(int &pos)
	{
		int i = 0,now = 1;
		while(s[i])
		{
			if(!ch[now][s(i)])
				ch[now][s(i)] = ++cnt;
			now = ch[now][s(i)];
			sum[now]++;
			i++;
		}
		pos = now;
	}
	void build()
	{
		h = 0,t = -1;
		q[++t] = 1;fail[1] = 0;
		while(h<=t)
		{
			int x = q[h++];
			for(int i = 0;i<26;i++)
			{
				int to = ch[x][i];
				if(!to)continue;
				int k = fail[x];
				while(!ch[k][i])k=fail[k];
				fail[to]=ch[k][i];
				q[++t] = to;
			}
		}
		for(int i = t;i>=0;i--)
			sum[fail[q[i]]]+=sum[q[i]];
	}
