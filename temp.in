{
	if(W[x]<=m)f[x][W[x]] = V[x];
	for(int i = h2[x];i;i = e[i].next)
		if(e[i].to!=fa)
			dp(e[i].to,x);
	for(int i = h2[x];i;i = e[i].next)
		for(int j = m;j>=0;j--)
			for(int k = 0;k<= j;k++)
				if(f[x][j-k]!=-1&&f[e[i].to][k]!=-1)
					f[x][j] = max(f[x][j],f[x][j-k]+f[e[i].to][k]);
}
