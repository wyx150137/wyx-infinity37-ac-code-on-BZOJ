void div_block()
{
	for(int i = 1;i<= n;i++)pos[i] = (i-1)/size+1;
	tot = pos[n];
	for(int i = 1;i<= tot;i++)st[i] = (i-1)*size+1,end[i] = i*size;
	end[tot] =n;
	for(int i = 1;i<= tot;i++)
		for(int j = st[i];j<=end[i];j++)
			cnt[i][num[j]]++;
